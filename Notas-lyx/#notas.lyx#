\begin_body

\begin_layout Part
Repaso Analisis
\end_layout

\begin_layout Section
Series
\end_layout

\begin_layout Subsection
Serie Geometrica
\end_layout

\begin_layout Standard
La serie geometrica 
\begin_inset Formula 
\[
\sum_{n=1}^{\infty}ar^{n-1}=a+ar+arÂ²+\dots
\]

\end_inset

es convergente si 
\begin_inset Formula $|r|<1$
\end_inset

 y su suma es 
\begin_inset Formula 
\[
\sum_{n=1}^{\infty}ar^{n-1}=\frac{a}{1-r}
\]

\end_inset

Si 
\begin_inset Formula $|r|\ge1$
\end_inset

,
 la serie es divergente
\end_layout

\begin_layout Subsection
Serie Armonica
\end_layout

\begin_layout Standard
La serie 
\begin_inset Formula 
\[
\sum_{n=1}^{\infty}\frac{1}{n}=1+\frac{1}{2}+\frac{1}{3}+\dots
\]

\end_inset

es divergente.
\end_layout

\begin_layout Subsection
Teorema de divergencia convergencia
\end_layout

\begin_layout Itemize
Si la serie 
\begin_inset Formula $\sum_{n=1}^{\infty}a_{n}$
\end_inset

 es 
\series bold
convergente
\series default
,
 entonces 
\begin_inset Formula $\lim_{n\to\infty}a_{n}=0$
\end_inset

.
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $\lim_{n\to\infty}a_{n}\neq0$
\end_inset

 o no existe,
 entonces la serie 
\begin_inset Formula $\sum_{n=1}^{\infty}a_{n}$
\end_inset

 es 
\series bold
divergente
\series default
.
\end_layout

\begin_layout Subsection
Serie 
\begin_inset Formula $\boldsymbol{p}$
\end_inset


\end_layout

\begin_layout Standard
La serie 
\begin_inset Formula $p$
\end_inset

 
\begin_inset Formula 
\[
\sum_{n=1}^{\infty}\frac{1}{n^{p}}
\]

\end_inset

es 
\series bold
convergente
\series default
 si 
\begin_inset Formula $p>1$
\end_inset

 y 
\series bold
divergente
\series default
 si 
\begin_inset Formula $p\le1$
\end_inset

.
\end_layout

\begin_layout Subsection
Prueba por comparacion
\end_layout

\begin_layout Standard
Supongamos que 
\begin_inset Formula $\sum a_{n}$
\end_inset

 y 
\begin_inset Formula $\sum b_{n}$
\end_inset

 son series con terminos positivos.
 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $\sum b_{n}$
\end_inset

 es 
\series bold
convergente
\series default
 y 
\begin_inset Formula $a_{n}\le b_{n}$
\end_inset

 para toda 
\begin_inset Formula $n$
\end_inset

,
 entonces 
\begin_inset Formula $\sum a_{n}$
\end_inset

 tambien es 
\series bold
convergente
\series default
.
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $\sum b_{n}$
\end_inset

 es 
\series bold
divergente
\series default
 y 
\begin_inset Formula $a_{n}\ge b_{n}$
\end_inset

 para toda 
\begin_inset Formula $n$
\end_inset

,
 entonces 
\begin_inset Formula $\sum a_{n}$
\end_inset

 tambien es 
\series bold
divergente.
\end_layout

\begin_layout Subsection
Prueba por comparacion del limite
\end_layout

\begin_layout Standard
Supongamos que 
\begin_inset Formula $\sum a_{n}$
\end_inset

 y 
\begin_inset Formula $\sum b_{n}$
\end_inset

 son series con terminos positivos.
 Si
\begin_inset Formula 
\[
\lim_{n\to\infty}\frac{a_{n}}{b_{n}}=c
\]

\end_inset

donde 
\begin_inset Formula $c$
\end_inset

 es un numero finito y 
\begin_inset Formula $c>0$
\end_inset

,
 entonces ambas series 
\series bold
convergen
\series default
 o ambas 
\series bold
divergen.
\end_layout

\begin_layout Subsection
Series alternantes
\end_layout

\begin_layout Standard
Si la serie alternante dada por 
\begin_inset Formula 
\[
\sum_{n=1}^{\infty}(-1)^{n-1}b_{n}
\]

\end_inset

cumple con 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $b_{n+1}\le b_{n}$
\end_inset

 para toda 
\begin_inset Formula $n$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lim_{n\to\infty}b_{n}=0$
\end_inset


\end_layout

\begin_layout Standard
entonces la serie es 
\series bold
convergente
\series default
.
\end_layout

\begin_layout Standard

\end_layout

